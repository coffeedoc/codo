!!!
%html
  != @JST['partials/head'](@)
  %body.list
    #content.tree.list
      %h1.full_list_header Modules List

      %nav
        %a{ target: '_self', href: 'class_list.html' } Classes
        %a{ target: '_self', href: 'module_list.html' } Modules
        %a{ target: '_self', href: 'method_list.html' } Methods
        %a{ target: '_self', href: 'file_list.html' } Files

      #search
        Search:
        %input{ type: 'text' }

      - createLevel = (modules) ->
        %ul
          - for module in modules
            %li
              - if module.href
                %a{ href: module.href, target: 'main' }= module.name
              - else
                %span
                  = module.name

            = createLevel(module.children) if module.children

      = createLevel @modules